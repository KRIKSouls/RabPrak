<Page x:Class="WaterSupply.Lists.HelperFolder.End"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WaterSupply.Lists.HelperFolder"
      xmlns:gif="http://wpfanimatedgif.codeplex.com"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="Конец обучения">

    <Grid Background="White">
        <TextBox FontFamily="Cambria Math" Text="Поздравляю! Вы успешно прошли обучение, теперь вы можете приступать к своей работе. Хорошего дня и удачи Вам!" Margin="184,247,178,260" TextWrapping="Wrap"/>
        <Border Background="#27659c" HorizontalAlignment="Center" RenderTransformOrigin="0,1" CornerRadius="20" Margin="0,457,0,120" Width="142">
            <Button x:Name="Next" Content="Закончить" HorizontalAlignment="Center" Click="Next_Click"  VerticalAlignment="Center" Width="122" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFFBE5E5" FontFamily="Cambria Math" Margin="10,0" Height="23"/>
        </Border>
    </Grid>
</Page>
